{
    "applicationInterfaceId": "API_ID",
    "propertyMappings": {
        "status": {
            "eventCount": "$state.eventCount+1",
            "airTemperature.C": "$event.d.DecimalType",
            "airTemperature.F": "$event.d.DecimalType * 1.8 + 32",
            "airTemperature.isLow": "$event.d.DecimalType < $state.airTemperature.lowest",
            "airTemperature.isHigh": "$event.d.DecimalType > $state.airTemeprature.highest",
            "airTemperature.lowest": "($event.d.DecimalType < $state.airTemperature.lowest) ? $event.d.DecimalType : $state.airTemperature.lowest",
            "airTemperature.highest": "($event.d.DecimalType < $state.airTemperature.highest) ? $event.d.DecimalType : $state.airTemperature.highest"
        }
    }
}
